<p id="notice"><%= notice %></p>

<h1>Wines Oenologists</h1>

<table>
  <thead>
    <tr>
      <th>Wine</th>
      <th>Oenologist</th>
      <th>Grade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wines_oenologists.each do |wines_oenologist| %>
      <tr>
        <td><%= wines_oenologist.wine %></td>
        <td><%= wines_oenologist.oenologist %></td>
        <td><%= wines_oenologist.grade %></td>
        <td><%= link_to 'Show', wines_oenologist %></td>
        <td><%= link_to 'Edit', edit_wines_oenologist_path(wines_oenologist) %></td>
        <td><%= link_to 'Destroy', wines_oenologist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wines Oenologist', new_wines_oenologist_path %>
